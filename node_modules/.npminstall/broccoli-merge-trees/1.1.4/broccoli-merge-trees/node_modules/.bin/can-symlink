#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../../can-symlink/1.0.0/can-symlink/can-symlink.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../../can-symlink/1.0.0/can-symlink/can-symlink.js" "$@"
  ret=$?
fi
exit $ret
